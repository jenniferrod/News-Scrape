<div class="container">
    <div class="row">
        {{#each articles}}
        <div class="card">
            <div class="card-body">
                <h2>{{this.title}}</h2>
                <p>{{this.summary}}</p>
                <a href="{{this.link}}">Read More</a>
            </div>
            <div>
                <button class="btn saved" data-id="{{this._id}}">Unsave</button>
                <button class="btn note" data-id="{{this._id}}">Add Note</button>
            </div>
        </div>
        {{/each}}
    </div>
</div>